<input type="hidden" id="report_sys_id" name="report_sys_id" value="$[report]" />
  <span id="report"/>
  <style>
	  #img_company_loader {
		  display:none;
		  position:absolute;
		  z-index:1000;
		  color: white;
	  }
	  #div_company_loader {
		  display:none;
		  position:fixed;
		  background:#000;
		  z-index:999;
	  }
  </style>
  <div id="div_company_loader">
  <i id="img_company_loader" class="fa fa-spinner fa-pulse fa-4x"></i>
 </div>
</j:jelly>]]></html>
